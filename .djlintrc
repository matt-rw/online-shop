{
  "profile": "django",
  "indent": 2,
  "max_line_length": 120,
  "max_attribute_length": 80,
  "format_attribute_template_tags": true,
  "preserve_blank_lines": true,
  "preserve_leading_space": false,
  "blank_line_after_tag": "load,extends,include",
  "blank_line_before_tag": "block,endblock",
  "close_void_tags": false,
  "format_css": true,
  "format_js": true,
  "ignore": "H006,H030,H031,T002",
  "exclude": "venv,.venv,migrations,node_modules,.git",
  "extend_exclude": "",
  "linter_output_format": "{code} {line}:{column} {message}",
  "custom_blocks": "",
  "custom_html": "",
  "indent_css": 2,
  "indent_js": 2,
  "require_pragma": false,
  "no_line_after_yaml": false,
  "use_gitignore": true
}
